generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model Vocabulary {
  id          Int      @id @default(autoincrement())
  term        String
  definition  String
  language    String
  createdAt   DateTime @default(now())
  exampleGerman  String?
  exampleEnglish String?
}
