generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgres"
}

model Vocabulary {
  id             Int      @id @default(autoincrement())
  term           String
  definition     String
  language       String
  createdAt      DateTime @default(now())
  exampleGerman  String?
  exampleEnglish String?
}
